{
    "name": "objectively-express-boilerplate",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "v9.3.0"
    },
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
