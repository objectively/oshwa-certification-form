{
    "name": "objectively-express-boilerplate",
    "version": "0.0.0",
    "private": true,
    "engines": {
        "node": "v9.3.0"
    },
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "test": "nyc --reporter=html --reporter=text mocha $(find test -name '*.js')",
        "lint": "eslint ./client/**/*.js",
        "lint-server": "eslint ./app.js ./services/*.js ./controllers/*.js ./models/*.js ./helpers/*.js ./routes/*.js",
        "prettier": "prettier --config ./.prettierrc --write ./client/**/*.js"
    },
    "dependencies": {
        "babelify": "^10.0.0",
        "basic-auth": "^2.0.0",
        "body-parser": "~1.18.2",
        "browser-sync": "^2.24.5",
        "browserify-middleware": "^8.1.1",
        "compression": "^1.7.2",
        "connect-browser-sync": "^2.1.0",
        "contentful": "^7.0.3",
        "contentful-management": "^5.1.4",
        "cookie-parser": "~1.4.3",
        "debug": "^4.0.1",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-handlebars": "^3.0.0",
        "express-minify": "^1.0.0",
        "express-minify-html": "^0.12.0",
        "express-validator": "^5.3.0",
        "helmet": "^3.12.1",
        "jquery": "^3.3.1",
        "marked": "^0.5.1",
        "morgan": "~1.9.0",
        "node-sass-middleware": "^0.11.0",
        "recaptcha2": "^1.3.2",
        "serve-favicon": "^2.5.0",
        "sinon": "^6.3.1",
        "supertest": "^3.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.1",
        "@babel/preset-env": "^7.0.0",
        "chai": "^4.1.2",
        "eslint": "^5.6.0",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-mocha": "^5.2.0",
        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-smells": "^1.0.1",
        "jsdom": "^12.0.0",
        "jshint": "^2.9.6",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.1",
        "nyc": "^13.0.1",
        "prettier": "^1.14.3",
        "source-map-support": "^0.5.9"
    }
