{
  "name": "objectively-express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "v9.3.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "mocha $(find spec -name '*.js')",
    "lint": "eslint ./client/**/*.js",
    "lint-server": "eslint ./app.js ./**/*.js",
    "prettier": "prettier --config ./.prettierrc --write ./client/**/*.js"
  },
  "dependencies": {
    "basic-auth": "^2.0.0",
    "body-parser": "~1.18.2",
    "browser-sync": "^2.24.5",
    "browserify-middleware": "^8.1.1",
    "compression": "^1.7.2",
    "connect-browser-sync": "^2.1.0",
    "contentful": "^6.1.1",
    "contentful-management": "^5.1.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.15.5",
    "express-handlebars": "^3.0.0",
    "express-minify": "^1.0.0",
    "express-minify-html": "^0.12.0",
    "express-validator": "^5.3.0",
    "helmet": "^3.12.1",
    "jquery": "^3.3.1",
    "marked": "^0.4.0",
    "morgan": "~1.9.0",
    "node-sass-middleware": "^0.11.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.1",
    "prettier": "1.13.7"
  }
}
