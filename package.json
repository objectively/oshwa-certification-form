        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-mocha": "^5.2.0",
        "eslint-plugin-prettier": "^2.6.2",
        "jsdom": "^11.11.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.1",
        "nyc": "^12.0.2",
        "prettier": "1.13.7",
        "source-map-support": "^0.5.9"
